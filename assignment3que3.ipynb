   "source": [
    "k=input()\n",
    "def upplowercount(s):\n",
    "    l,u = 0,0\n",
    "    for i in s:\n",
    "        if (i>='a'and i<='z'):\n",
    "            l=l+1                 \n",
    "        if (i>='A'and i<='Z'):\n",
    "            u=u+1   \n",
    "    return \"Lowercase letters count=\",l,\"uppercase letters count=\",u\n",
    "\n",
    "          \n",
    "print(upplowercount(k))"
   ]
